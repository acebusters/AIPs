{
  "name": "utxo-metrics",
  "description": "Monthly active utxo metrics lambda function",
  "version": "1.0.0",
  "contributors": [
    "Marin PetruniÄ‡ <marin@nodefactory.io>"
  ],
  "license": "MIT",
  "scripts": {
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "tsc --sourceMap false --outDir dist",
    "build:watch": "npm run build -w",
    "run": "node ./bin/run.js",
    "pack": "npm run build && claudia pack --output handler.zip --force --use-local-dependencies",
    "pack:run": "npm run pack -- --post-package-script run",
    "compile": "tsc",
    "lint": "tslint src/**/*.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/aws-lambda": "^8.10.23",
    "@types/node": "^8.10.42",
    "claudia": "^5.4.2",
    "dotenv": "^5.0.1",
    "runjs": "^4.4.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "engines": {
    "node": "8.10"
  }
}
